<h1><%= t(".title") %></h1>

<div id="task_sets" class="space-y-4">
  <% @task_sets.each do |task_set| %>
    <%= link_to task_set, class: "card card-link" do %>
      <div class="px-6 py-4">
        <%= render task_set %>
      </div>
    <% end %>
  <% end %>
</div>

<%= link_to "New task set", new_task_set_path, class: "btn btn-primary mt-4" %>
